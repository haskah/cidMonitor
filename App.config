<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    
    <appSettings>
        <!-- CloudVoice API Ayarlar覺 -->
        <add key="CloudVoiceApiUrl" value="https://your-cloud-voice-api.com/api/calls/latest" />
        <add key="CloudVoiceApiKey" value="" />
        <add key="WebhookPort" value="8080" />
        <add key="PollingInterval" value="3000" />
        
        <!-- COM Port Ayarlar覺 -->
        <add key="ComPort" value="COM4" />
        <add key="BaudRate" value="1200" />
        
        <!-- Log Ayarlar覺 -->
        <add key="EnableFileLogging" value="true" />
        <add key="LogLevel" value="Info" />
        
        <!-- Test Ayarlar覺 -->
        <add key="EnableTestMode" value="true" />
        <add key="TestPhoneNumber" value="+905551234567" />
    </appSettings>
    
    <system.net>
        <defaultProxy enabled="true" useDefaultCredentials="true">
            <proxy autoDetect="true" />
        </defaultProxy>
    </system.net>
    
    <runtime>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <dependentAssembly>
                <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
                <bindingRedirect oldVersion="0.0.0.0-13.0.0.0" newVersion="13.0.0.0" />
            </dependentAssembly>
        </assemblyBinding>
    </runtime>
</configuration> 